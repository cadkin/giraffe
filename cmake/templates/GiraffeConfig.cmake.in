@PACKAGE_INIT@

message(STATUS "Found @PROJECT_NAME@ @CMAKE_PROJECT_VERSION@")
list(APPEND CMAKE_MESSAGE_INDENT "    ")

foreach(CURRENT_COMPONENT ${@PROJECT_NAME@_FIND_COMPONENTS})
    set(CURRENT_CONFIG_FILE "${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@-${CURRENT_COMPONENT}-targets.cmake")

    if(EXISTS "${CURRENT_CONFIG_FILE}")
        message(STATUS "Found component ${CURRENT_COMPONENT} at ${CURRENT_CONFIG_FILE}")
        include("${CURRENT_CONFIG_FILE}")
        set(@PROJECT_NAME@_${CURRENT_COMPONENT}_FOUND TRUE)
    else()
        message(WARNING "Could not find component ${CURRENT_COMPONENT} at ${CURRENT_CONFIG_FILE}")
        set(@PROJECT_NAME@_${CURRENT_COMPONENT}_FOUND FALSE)
    endif()
endforeach()

list(POP_BACK CMAKE_MESSAGE_INDENT)

set(@PROJECT_NAME@_FOUND TRUE)
check_required_components(@PROJECT_NAME@)
